@model List<SpadCompanyPanel.Core.Models.Partner>
<section class="clients-section">
    <div class="auto-container">
        <div class="sponsors-outer">
            <!--Sponsors Carousel-->
            <ul class="sponsors-carousel owl-carousel owl-theme">
                @foreach (var item in Model)
                {
                    <li class="slide-item">
                        <figure class="image-box">
                            <a href="javasript:void(0)">
                                <img src="/Files/PartnersImages/@item.Image"
                                     data-src="/Files/PartnersImages/@item.Image" alt="@item.Title">
                            </a>
                        </figure>
                    </li>
                }
            </ul>
        </div>
    </div>
</section>
